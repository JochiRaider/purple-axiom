{
  "contract_version": "0.1.0",
  "schemas": [
    {
      "name": "manifest.schema.json",
      "purpose": "Run-level manifest (runs/<run_id>/manifest.json)."
    },
    {
      "name": "ground_truth.schema.json",
      "purpose": "Per-line schema for ground_truth.jsonl."
    },
    {
      "name": "criteria_pack_manifest.schema.json",
      "purpose": "Criteria pack manifest snapshot (criteria/manifest.json)."
    },
    {
      "name": "criteria_entry.schema.json",
      "purpose": "Per-line schema for criteria/criteria.jsonl."
    },
    {
      "name": "criteria_result.schema.json",
      "purpose": "Per-line schema for criteria/results.jsonl."
    },
    {
      "name": "ocsf_event_envelope.schema.json",
      "purpose": "Minimal required OCSF + provenance for normalized events."
    },
    {
      "name": "detection_instance.schema.json",
      "purpose": "Per-line schema for detections/detections.jsonl."
    },
    {
      "name": "summary.schema.json",
      "purpose": "Scoring summary for scoring/summary.json."
    },
    {
      "name": "duckdb_conformance_report.schema.json",
      "purpose": "DuckDB conformance report output (reports/duckdb_conformance_report.json)."
    },
    {
      "name": "telemetry_validation.schema.json",
      "purpose": "Telemetry validation summary (runs/<run_id>/logs/telemetry_validation.json)."
    },
    {
      "name": "mapping_profile_snapshot.schema.json",
      "purpose": "Normalization mapping profile snapshot (normalized/mapping_profile_snapshot.json)."
    },
    {
      "name": "mapping_profile_input.schema.json",
      "purpose": "Mapping profile input YAML validation (mappings/normalizer/ocsf/**)."
    },
    {
      "name": "mapping_coverage.schema.json",
      "purpose": "Optional mapping coverage stats for normalized/mapping_coverage.json."
    },
    {
      "name": "bridge_router_table.schema.json",
      "purpose": "Sigma logsource router table snapshot (bridge/router_table.json)."
    },
    {
      "name": "bridge_mapping_pack.schema.json",
      "purpose": "Sigma-to-OCSF bridge mapping pack snapshot (bridge/mapping_pack_snapshot.json)."
    },
    {
      "name": "bridge_compiled_plan.schema.json",
      "purpose": "Sigma-to-OCSF bridge compiled plan per rule (bridge/compiled_plans/<rule_id>.plan.json)."
    },
    {
      "name": "bridge_coverage.schema.json",
      "purpose": "Bridge coverage summary (bridge/coverage.json)."
    },
    {
      "name": "cleanup_verification.schema.json",
      "purpose": "Cleanup verification results per action (runs/<run_id>/runner/actions/<action_id>/cleanup_verification.json)."
    },
    {
      "name": "requirements_evaluation.schema.json",
      "purpose": "Per-action requirements evaluation output (runs/<run_id>/runner/actions/<action_id>/requirements_evaluation.json)."
    },
    {
      "name": "runner_executor_evidence.schema.json",
      "purpose": "Per-action runner executor evidence (runs/<run_id>/runner/actions/<action_id>/executor.json)."
    },
    {
      "name": "cache_provenance.schema.json",
      "purpose": "Run-level cache provenance evidence (runs/<run_id>/runner/cache_provenance.json)."
    },
    {
      "name": "principal_context.schema.json",
      "purpose": "Run-level principal context evidence (runs/<run_id>/runner/principal_context.json)."
    },
    {
      "name": "side_effect_ledger.schema.json",
      "purpose": "Per-action append-only side-effect ledger (runs/<run_id>/runner/actions/<action_id>/side_effect_ledger.json)."
    },
    {
      "name": "state_reconciliation_report.schema.json",
      "purpose": "Per-action state reconciliation report (runs/<run_id>/runner/actions/<action_id>/state_reconciliation_report.json)."
    },
    {
      "name": "pcap_manifest.schema.json",
      "purpose": "Manifest enumerating pcap capture files for a run (placeholder contract; v0.1)."
    },
    {
      "name": "netflow_manifest.schema.json",
      "purpose": "Manifest enumerating netflow capture/log files for a run (placeholder contract; v0.1)."
    },
    {
      "name": "range_config.schema.json",
      "purpose": "Range configuration input (range.yaml)."
    }
  ],
  "notes": [
    "Most schemas are strict (additionalProperties=false) and provide an 'extensions' object for forward-compatible additions.",
    "OCSF envelope is permissive (additionalProperties=true) to allow full OCSF payloads and vendor extensions while requiring provenance."
  ]
}
