{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "parquet_schema_snapshot",
  "type": "object",
  "additionalProperties": false,
  "required": [
    "contract_version",
    "schema_id",
    "schema_version",
    "columns"
  ],
  "properties": {
    "contract_version": {
      "const": "0.1.0"
    },
    "schema_id": {
      "type": "string",
      "minLength": 1
    },
    "schema_version": {
      "type": "string",
      "minLength": 1
    },
    "columns": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "name",
          "physical_type",
          "logical_type",
          "nullable"
        ],
        "properties": {
          "name": {
            "type": "string",
            "minLength": 1
          },
          "physical_type": {
            "type": "string",
            "minLength": 1
          },
          "logical_type": {
            "type": "string",
            "minLength": 1
          },
          "nullable": {
            "type": "boolean"
          }
        }
      }
    },
    "aliases": {
      "type": "object",
      "additionalProperties": {
        "type": "string",
        "minLength": 1
      }
    }
  }
}
